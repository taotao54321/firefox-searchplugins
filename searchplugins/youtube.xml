<?xml version="1.0" encoding="UTF-8"?>
<OpenSearchDescription xmlns="http://a9.com/-/spec/opensearch/1.1/"
                       xmlns:moz="http://www.mozilla.org/2006/browser/search/">
  <ShortName>YouTube</ShortName>
  <Description>YouTube</Description>
  <InputEncoding>UTF-8</InputEncoding>
  <Image width="16" height="16">https://www.google.com/s2/favicons?domain=youtube.com</Image>
  <Url method="GET" type="text/html"
       template="https://www.youtube.com/results">
    <Param name="search_query" value="{searchTerms}"/>
  </Url>
  <moz:SearchForm>https://www.youtube.com/</moz:SearchForm>
</OpenSearchDescription>
